

public class GeometryExamples {
	public static void main(String[] args) {
		Triangle t = new Triangle(
				new Vertices2D(3, 2),
				new Vertices2D(-12, -3),
				new Vertices2D(30, 25));
		System.out.println(t);
		
		Rectangle r = new Rectangle(
				new Vertices2D(3, 2),
				12, 
				7.5);
		System.out.println(r);
		
		Circle c = new Circle(
				new Vertices2D(3, -2),
				3.8);
		System.out.println(c);
	}
}
